	author = {Talbot, Denis and Diop, Awa and MÃ©sidor, Miceline and Chiu, Yohann and Sirois, Caroline and Spieker, Andrew J. and Pariente, Antoine and Noize, Pernelle and Simard, Marc and Luque Fernandez, Miguel Angel and Schomaker, Michael and Fujita, Kenji and Gnjidic, Danijela and Schnitzer, Mireille E.},
	year = {2025},

	note = {Publisher: SAGE Publications},
	keywords = {Rubin's Rules},
	pages = {227--241},
	file = {SAGE PDF Full Text:C\:\\Users\\anbe6\\Zotero\\storage\\4NKBTMK5\\Royston - 2004 - Multiple Imputation of Missing Values.pdf:application/pdf},
}

